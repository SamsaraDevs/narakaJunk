actor ShotgunDrop2: SamsaraSlotTwoDrop replaces ShotgunDrop
{
    States
    {
      Spawn:
        SHOT A -1
        stop
		
      Pickup:
		TNT1 A 0 A_JumpIfInventory("VillainAmmoHeavy",0,"PickupFailure")
	    TNT1 A 0 A_JumpIfInventory("IsNarakaClass",1,"PickupVillain")
        TNT1 A 0 A_JumpIf((ACS_ExecuteWithResult(229, 2) & 3) == 1, "PickupSuccess")
        goto PickupFailure
		
	  PickupVillain:
		TNT1 A 0 A_PlaySound("ammo/shellsmall")
	    TNT1 A 0 A_GiveInventory("VillainAmmoHeavy",4)
		stop
    }
}

actor ChingunDrop2: SamsaraSlotFourDrop replaces ChingunDrop
