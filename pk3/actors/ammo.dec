actor Ammo1Small2 : Ammo1Small replaces Clip// replaces Clip
{
    States
    {
      Pickup:
		TNT1 A 0 A_JumpIfInventory("VillainAmmoLight",0,"PickupFail")
		TNT1 A 0 A_JumpIfInventory("IsNarakaClass",1,"PickupVillain")
        TNT1 A 0 A_JumpIfInventory("MarathonClass", 0, "PickupMarathon")
		TNT1 A 0 A_JumpIfInventory("Clip",0,"PickupFail")
        TNT1 A 0 A_GiveInventory("Clip", 10)
        TNT1 A 0 A_JumpIfInventory("QuakeClass", 0, "PickupQuake")
        stop
		
	  PickupVillain:
	    TNT1 A 0 A_GiveInventory("VillainAmmoLight",10)
		stop
    }
}

