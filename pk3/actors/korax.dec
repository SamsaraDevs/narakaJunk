		KORX H 1 A_UnsetShootable//A_Die
	  //Death:
        TNT1 A 0 A_Stop
        TNT1 A 0 A_ChangeFlag("DONTGIB",1)
        TNT1 A 0 A_PlaySoundEx("chex/zorchhuge","soundslot5",0,0)
        KORX H 10 A_SpawnItem("ZorchEffectHuge")
        KORX H 10 A_SetTranslucent(0.9,0)
        TNT1 A 0 A_PlaySoundEx("chex/koraxzorch","soundslot6",0,2)
        KORX H 10 A_SetTranslucent(0.8,0)
        TNT1 A 0 A_NoBlocking
        KORX H 10 A_SetTranslucent(0.7,0)
        KORX H 10 A_SetTranslucent(0.6,0)
        KORX H 10 A_SetTranslucent(0.5,0)
	Death:
        TNT1 A 0 Radius_Quake(4,16,0,16,0)
		TNT1 A 0 A_Explode(198,256,0)
        TNT1 A 0 A_KoraxBonePop
        KORX H 10 A_SetTranslucent(0.4,0)
        KORX H 10 A_SetTranslucent(0.3,0)
        KORX H 10 A_SetTranslucent(0.2,0)
        KORX H 10 A_SetTranslucent(0.1,0)
        ACLO E 1 A_SetTranslucent(1,0)
        ACLO E -1
        stop
    }
}

ACTOR FrozenKorax : Korax
{
    Radius 1
    Height 1
    +NOCLIP
    -SOLID
	+FORCERADIUSDMG
	-SHOOTABLE
	+PAINLESS
	Scale 0.75
	Obituary "%k \crleft\c- %o \crone last surprise.\c-"
    States
    {
      Spawn:
	  See:
		KORX H 1 A_UnsetShootable//A_Die
	  //Death:
        TNT1 A 0 A_NoBlocking
        KORX Z 0 
        KORX Z 0 A_PlaySoundEx("freeze/korax","soundslot6",0,0)
        KORX Z 0 A_GenericFreezeDeath
        KORX Z 80 A_UnsetSolid
	Death:
        TNT1 A 0 Radius_Quake(4,8,0,16,0)
		TNT1 A 0 A_Explode(198,256,0)
        TNT1 A 0 A_KoraxBonePop
        KORX Z 60 A_UnsetSolid
        KORX Z 0 A_PlaySoundEx("freeze/break","soundslot6",0,0)
        KORX HHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHHH 0 A_SpawnItemEx("IceChunk2",random(-1,1),random(-1,1),random(16,96),random(-2,2),random(-2,2),random(5,15))
        KORX Z 0 A_IceGuyDie
        KORX Z 1 A_IceGuyDie
        ACLO A -1
        stop
    }
}

ACTOR KoraxSpirit2 : KoraxSpirit replaces KoraxSpirit
{
  +NOCLIP
  -ACTIVATEPCROSS
  -ACTIVATEIMPACT
  +CLIENTSIDEONLY
  +BRIGHT
  RenderStyle Add
  Alpha 0.75
  States
  {
  Spawn:
    SPIR A 5 A_KSpiritRoam
	TNT1 A 0 A_Jump(13,"Scream")
  Spawn2:
	SPIR B 5 A_KSpiritRoam
	TNT1 A 0 A_Jump(13,"Scream2")
    goto Spawn
  Scream:
	TNT1 A 0 A_PlaySoundEx("korax/spiritscream","voice",0,0)
	goto Spawn2
  Scream2:
	TNT1 A 0 A_PlaySoundEx("korax/spiritscream","voice",0,0)
	goto Spawn
}
}

actor "Ethereal Wraith Magic" : InstantSwitchWeapon
{
    Weapon.SlotNumber 1
	Weapon.AmmoType "VillainAmmoHeavy"
	Weapon.AmmoGive 0
	Weapon.AmmoUse 0
    +AMMO_OPTIONAL
    +ALT_AMMO_OPTIONAL
	+NOALERT
    Weapon.Kickback 100
	Weapon.UpSound "korax/swap"
    States
    {
      Ready:
        RMKX A 1 A_WeaponReady
        loop
		
	  Nope:
        RMKX A 35 A_WeaponReady(6)
        goto ready

      Fire: // Ethereal Wraith
        RMKX A 1 A_AlertMonsters
		TNT1 A 0 A_JumpIfInventory("VillainAmmoHeavy",1,1)
		goto Nope
		RMKX B 2 A_GunFlash
		TNT1 A 0 A_PlayWeaponSound("Korax/Attack")
		TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"CoopWraithFire")
		TNT1 A 0 A_JumpIfInventory("TLMSModeOn",1,"TLMSWraithFire")
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,-10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,-20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,-20,-5,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1",random(-1,1),0,20,-5,0,random(0,2))
		TNT1 A 0 A_TakeInventory("VillainAmmoHeavy",2)
		RMKX HIIJJJK 3
		RMKX A 14
        goto ready
		
	  TLMSWraithFire:
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,-10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,-20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,-20,-5,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1TLMS",random(-1,1),0,20,-5,0,random(0,2))
		TNT1 A 0 A_TakeInventory("VillainAmmoHeavy",2)
		RMKX HIIJJJK 3
		RMKX A 14
        goto ready
		
	  CoopWraithFire:
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,-10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,10,25,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,-20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,20,10,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,-20,-5,0,random(0,2))
		TNT1 A 0 A_FireCustomMissile("KoraxWraithFX1Coop",random(-1,1),0,20,-5,0,random(0,2))
		TNT1 A 0 A_TakeInventory("VillainAmmoHeavy",2)
		RMKX HIIJJJK 3
		RMKX A 14
        goto ready
		
	  AltFire:
        RMKX A 1 A_AlertMonsters
		RMKX B 1 A_GunFlash
		TNT1 A 0 A_JumpIfInventory("CoopModeOn",1,"CoopAltFire")
		TNT1 A 0 A_JumpIfInventory("TLMSModeOn",1,"CoopAltFire")
		TNT1 A 0 A_FireCustomMissile("KoraxMeleeSplosion",0,0)
		RMKX B 1 A_GunFlash
		TNT1 A 0 A_PlayWeaponSound("ClericFlameExplode")
		RMKX CDDEEEF 2
		RMKX A 5
		goto Ready
		
	  CoopAltFire:
		TNT1 A 0 A_FireCustomMissile("KoraxMeleeSplosionCoop",0,0)
		RMKX B 1 A_GunFlash
		TNT1 A 0 A_PlayWeaponSound("ClericFlameExplode")
		RMKX CDDEEEF 2
		RMKX A 5
		goto Ready
	}
}

